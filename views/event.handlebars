<div class="container">
    <div class="panel panel-default box" style="margin-top: 25px">
            <div class="panel-heading" style="margin-bottom: 10px"><h3>Event Details</h3></div>
		        <div class="panel-body">
		            <h2>{{event.name}}</h2>
		            <h5>{{event.date}}</h5>
		            <h5 style="margin-bottom:25px">{{event.duration}} minutes</h5>
		            <p style="margin-bottom:25px">{{event.desc}}</p>
		            <h5>Created On: {{event.created_at}}</h5>
		        </div>
		    </div>
		</div>