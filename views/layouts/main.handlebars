	<script>
	function musictoggle(){
		var audioElem = document.getElementById('audio');
		var buttontext = document.getElementById('musictoggle');
		if (audioElem.paused) {
   			audioElem.play();
   			buttontext.className="fa fa-pause fa-fw mustoggle";
  		} else {
    		audioElem.pause();
    		buttontext.className="fa fa-play fa-fw mustoggle";
    	}
    }
	</script>
